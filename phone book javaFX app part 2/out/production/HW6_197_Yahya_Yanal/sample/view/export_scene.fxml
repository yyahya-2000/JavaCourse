<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="345.0" prefWidth="371.0" style="-fx-background-color: #3D4956;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.ExportController">
   <children>
      <Label layoutX="18.0" layoutY="135.0" text="Directory:" textFill="WHITE">
         <font>
            <Font name="Times New Roman" size="25.0" />
         </font>
      </Label>
      <TextField id="FilePathExportField" fx:id="filePathExportField" editable="false" layoutX="11.0" layoutY="173.0" prefHeight="29.0" prefWidth="261.0" style="-fx-background-radius: 20px;">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font></TextField>
      <ButtonBar layoutX="185.0" layoutY="291.0" prefHeight="40.0" prefWidth="175.0">
         <buttons>
            <Button id="CancelExportButton" fx:id="cancelExportBtn" lineSpacing="10.0" mnemonicParsing="false" onAction="#cancelExportingAction" style="-fx-background-color: #D35400; -fx-background-radius: 15px;" text="Cancel">
               <font>
                  <Font name="Times New Roman Bold" size="15.0" />
               </font>
            </Button>
            <Button id="ExportButton" fx:id="exportBtn" mnemonicParsing="false" onAction="#exportPhones" style="-fx-background-color: #D35400; -fx-background-radius: 15px;" text="Export">
               <font>
                  <Font name="Times New Roman Bold" size="15.0" />
               </font>
            </Button>
         </buttons>
      </ButtonBar>
      <Button fx:id="browseBtn" layoutX="283.0" layoutY="175.0" mnemonicParsing="false" onAction="#choiceDirectoryBtnAction" prefHeight="28.0" prefWidth="82.0" style="-fx-background-color: #D35400; -fx-background-radius: 15px;" text="Browse">
         <font>
            <Font name="Times New Roman Bold" size="15.0" />
         </font>
      </Button>
      <Label layoutX="13.0" layoutY="220.0" text="File Name:" textFill="WHITE">
         <font>
            <Font name="Times New Roman" size="25.0" />
         </font>
      </Label>
      <TextField id="FileNameExportField" fx:id="fileNameExportField" layoutX="11.0" layoutY="257.0" prefHeight="34.0" prefWidth="349.0" style="-fx-background-radius: 20px;" text=".csv">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </TextField>
      <ImageView fitHeight="55.0" fitWidth="62.0" layoutX="154.0" layoutY="73.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../res/export_csv.png" />
         </image>
      </ImageView>
      <Pane layoutY="56.0" prefHeight="7.0" prefWidth="356.0" style="-fx-background-color: #000000;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <AnchorPane prefHeight="55.0" prefWidth="371.0" style="-fx-background-color: #D35400;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Label alignment="CENTER" layoutX="81.0" layoutY="15.0" text="Export Contacts" AnchorPane.leftAnchor="81.0" AnchorPane.rightAnchor="80.0">
               <font>
                  <Font name="Times New Roman" size="27.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
